#include<stdio.h>
#include<math.h>
int main(){
	int i;
	char a[290];
	char b[290];
	scanf("%s",a);
	for(i=0;i<255;i++)
		switch(a[i]){
			case 'A':b[i]='T';break;
			case 'T':b[i]='A';break;
			case 'G':b[i]='C';break;
			case 'C':b[i]='G';break;
			default: b[i] = '\0';  
            break;      
	}
	printf("%s",b);
}
